    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0"
