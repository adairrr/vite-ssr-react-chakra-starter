    "cross-fetch": "^3.1.5",
    "framer-motion": "^6.5.1",
    "node-fetch": "^3.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-streaming": "^0.2.17",
    "vite-plugin-ssr": "^0.4.12"
